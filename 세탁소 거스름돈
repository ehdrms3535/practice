a=int(input())
for i in range(a):
    b=int(input())
    c,d,e,f,g=0,0,0,0,0
    if b>=25:
        c=b//25
        d=b%25
        e=d//10
        d=d%10
        f=d//5
        d=d%5
        g=d//1
        print(c,e,f,g)
    elif b>=10:
        e=b//10
        d=b%10
        f=d//5
        d=d%5
        g=d//1
        print(c,e,f,g)
    elif b>=5:
        f=b//5
        d=b%5
        g=d//1
        print(c,e,f,g)
    else:
        g=b//1
        print(c,e,f,g)
